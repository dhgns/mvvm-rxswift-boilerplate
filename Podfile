# platform :ios, '9.0'

target 'mvvm-rxswift-boilerplate' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
    pod 'RxSwift', '~> 4'
    pod 'RxCocoa', '~> 4'
    pod 'Alamofire', :git => 'https://github.com/Alamofire/Alamofire.git', :branch => 'alamofire5'

  # Pods for mvvm-rxswift-boilerplate

  target 'mvvm-rxswift-boilerplateTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'mvvm-rxswift-boilerplateUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
